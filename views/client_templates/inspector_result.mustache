<label class="inspector-title">query result<br>{{ user }}</label>
<div class="sql-controls">
  <input type="checkbox" id="sql-toggle-{{user}}-{{name}}" class="sql-toggle" checked>
  <label for="sql-toggle-{{user}}-{{name}}" title="Show the SQL that generated this query">SQL</label>
</div>
<button class="close-inspector" data-user="{{ user }}" data-name="{{ name }}">&times;</button>
<button class="expander"></button>
<table class="object-details">
  <thead>
    <tr>
      <th class="detail-rowindex number">#</td>
      {{# columns }}
        <th class="detail-col">{{ . }}</th>
      {{/ columns }}
    </tr>
  </thead>
  <tbody>
    {{# rows }}
    <tr>
      <td class="detail-rowindex number">{{ rowIndex }}</td>
      {{# rowData }}
        <td class="detail-rowdata {{ type }}">{{{ value }}}</td>
      {{/ rowData }}
    </tr>
    {{/ rows }}
  </tbody>
</table>
<aside class="query-info"><code class="sql">{{ sql }}</code></aside>
